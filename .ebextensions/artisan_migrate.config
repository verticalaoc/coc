container_commands:
    01-optimize:
        command: "export COMPOSER_HOME=/root && /usr/bin/composer.phar self-update && /usr/bin/composer.phar dump-autoload --optimize"
    02-composer-update:
        command: "php composer update"
    02-env-cleanup:
        command: "php composer dump-autoload; php ./artisan optimize --force;php ./artisan config:cache;php ./artisan route:cache"
    03-migration:
        command: "php ./artisan migrate:rollback ; php ./artisan migrate --force"

